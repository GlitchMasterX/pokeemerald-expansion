    waitbuttonpress
    playmoncry(SPECIES_MEWTWO,CRY_MODE_WEAK)
    applymovement(0xFE,moves(shake_horizontal*3))
    waitmovement(0)
    playse(SE_PIN)
    applymovement(0xFE,moves(emote_exclamation_mark))
    speakername("Mewtwo")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_MEWTWO}{EMOTE_SHOCK}{COLOR LIGHT_RED}{SHADOW LIGHT_GRAY}Wait."))
    waitmessage
    closemessage
    speakername("Mewtwo")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_MEWTWO}{EMOTE_SHOCK}{COLOR LIGHT_RED}{SHADOW LIGHT_GRAY}{PLAY_BGM}{MUS_BW12_107}\pSomething about him… is not normal.\pI…… can't sense…… anything from him yet…{PAUSE_UNTIL_PRESS} my instincts are screaming at me to run."))
    waitmessage
    closemessage
    speakername("???")
    msgbox(format("{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}Human life is an illusion. Even at its longest, it is but a hundred fleeting years.\pBeauty withers, strength decays, every embrace turns to dust. Yet still, humans worship appearances, clinging to flesh as though it were eternal.\pThat… is not love…{PAUSE_UNTIL_PRESS} it is obsession."))  
    waitmessage
    applymovement(4,moves(walk_slow_down))
    applymovement(0xFE,moves(walk_fast_up walk_fast_left face_up))
    waitmovement(0)
    playmoncry(SPECIES_MEWTWO,CRY_MODE_ROAR_2)
    speakername("Mewtwo")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_MEWTWO}{EMOTE_MAD}{COLOR LIGHT_RED}{SHADOW LIGHT_GRAY}……"))
    waitmessage
    applymovement(0xFE,moves(shake_horizontal*5))
    speakername("Mewtwo")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_MEWTWO}{EMOTE_MAD}(Am I scared?……{PAUSE_UNTIL_PRESS} No, It's not possible.)"))
    waitmessage
    closemessage
    speakername("???")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_NIHIL}{EMOTE_NORMAL}{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}Tell me…… if the body is so precious, then why does it rot in the earth, while only the soul ascends to heaven?"))  
    waitmessage
    closemessage
    applymovement(4,moves(walk_slow_down))
    waitmovement(0)
    speakername("???")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_NIHIL}{EMOTE_NORMAL}{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}She died, and the world moved on."))  
    applymovement(4,moves(face_left))
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_NIHIL}{EMOTE_NORMAL}{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}Graves are not for the dead.\pThey are for the living…{PAUSE_UNTIL_PRESS} to lie to themselves.\pThat is the harsh reality of the world yet…{PAUSE_UNTIL_PRESS} I find it beautiful."))  
    waitmessage
    closemessage
    playse(SE_PIN)
    applymovement(0xFF,upset)
    speakername("Xylar")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_ANGRY}{RESET_FONT}{COLOR BLUE}{SHADOW LIGHT_GRAY}Beautiful…?\pYou dare stand over my mother's grave and call her death {COLOR RED}{SHADOW LIGHT_GRAY}beautiful!{COLOR BLUE}{SHADOW LIGHT_GRAY}\pI'll kill you where you stand!!"))
    closemessage
    playse(SE_PIN)
    applymovement(0xFE,moves(emote_exclamation_mark))
    speakername("Mewtwo")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_MEWTWO}{EMOTE_MAD}(Telepathy)\p{COLOR LIGHT_RED}{SHADOW LIGHT_GRAY}Xylar! Stop."))
    waitmessage
    closemessage
    applymovement(4,moves(face_down delay_8 walk_slow_down))
    waitmovement(0)
    speakername("???")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_NIHIL}{EMOTE_NORMAL}{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}Do not mistake me as your enemy, for you do not interest me in any way."))  
    waitmessage
    closemessage
    applymovement(4,moves(delay_8 face_up delay_8 walk_slow_up))
    waitmovement(0)
    speakername("???")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_NIHIL}{EMOTE_NORMAL}{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}I came only to witness the grave of one who once stood against me\pand who, for a fleeting moment,{PAUSE_UNTIL_PRESS} shone brighter than all others."))  
    waitmessage
    closemessage
    applymovement(4,moves(face_down))
    speakername("???")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_NIHIL}{EMOTE_NORMAL}{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}But brightness is fragile and so is anger.\pYou cling to anger.{PAUSE_UNTIL_PRESS} That is natural."))  
    waitmessage
    closemessage
    speakername("???")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_NIHIL}{EMOTE_NORMAL}{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}Live as you wish.\pBurn, break, or build…… it won't matter to me."))  
    waitmessage
    closemessage
    setweather(WEATHER_FOG_DIAGONAL)
    doweather
    playse(SE_M_BLIZZARD)
    applymovement(4,moves(walk_slow_up))
    applymovement(5,distortionportalopen)
    speakername("???")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_NIHIL}{EMOTE_NORMAL}{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}In the end, all things return to nothing."))  
    waitmessage
    closemessage
    applymovement(4,moves(walk_slow_up*2))
    removeobject(4)
    playse(SE_FU_ZAKU)
    speakername("???")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_NIHIL}{EMOTE_NORMAL}{FONT_SMALL}{COLOR DARK_GRAY}{SHADOW LIGHT_GRAY}That is the truth I carry."))  
    waitmessage
    closemessage
    waitmovement(5)
    playse(SE_M_TELEPORT)
    applymovement(5,distortionportalclose)
    waitmovement(0)
    removeobject(5)
    playbgm(MUS_DP_TITLE,FALSE)
    setweather(WEATHER_NONE)
    doweather
    applymovement(0xFE,moves(shake_vertical*3))
    playmoncry(SPECIES_MEWTWO,CRY_MODE_WEAK)
    playse(SE_PIN)
    applymovement(0xFF,exclam)
    speakername("Xylar")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_SHOCK}{COLOR BLUE}{SHADOW LIGHT_GRAY}Mewtwo……?!\pWhat happened…?"))
    closemessage
    playmoncry(SPECIES_MEWTWO,CRY_MODE_WEAK)
    speakername("Mewtwo")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_MEWTWO}{EMOTE_NORM}\p{COLOR LIGHT_RED}{SHADOW LIGHT_GRAY}That person……{PAUSE_UNTIL_PRESS} His presence was overwhelming…\pIt felt as if vast shadows were moving behind him…\pSilent, watching.…\pSomething……{PAUSE_UNTIL_PRESS}ancient, boundless, and suffocating."))
    waitmessage
    closemessage
    applymovement(0xFE,moves(face_down))
    speakername("Mewtwo")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_MEWTWO}{EMOTE_NORM}{COLOR LIGHT_RED}{SHADOW LIGHT_GRAY}Whoever he was…… we should avoid him until we grow strong.\pBecause if the presence I felt was truly his……{PAUSE_UNTIL_PRESS} then we stand no chance."))
    waitmessage
    closemessage
    applymovement(0x7F,moves(walk_slow_down*3))
    waitmovement(0)
    special(RemoveCameraObject)
   applymovement(0xFF,moves(face_down))
   speakername("Xylar")
    msgbox(format("Do you think Mewtwo was right? Were there really… shadows?"),MSGBOX_YESNO)
if(var(VAR_RESULT)==YES){
    speakername("Xylar")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_NORM}I see…… So, you believe Mewtwo's instincts."),MSGBOX_AUTOCLOSE)
}
else{
    speakername("Xylar")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_NORM}I see…… So, you don't believe Mewtwo's instincts.\pI think otherwise."),MSGBOX_AUTOCLOSE)
}

    speakername("Xylar")
    msgbox(format("Then… what about his words?\pAbout life, beauty, and death… do you believe them?"),MSGBOX_YESNO)
if(var(VAR_RESULT)==YES){
    speakername("Xylar")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_SHOCK}You do?……{PAUSE_UNTIL_PRESS} That's unexpected, but I understand."),MSGBOX_AUTOCLOSE)    
}
else{
    speakername("Xylar")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_NORM}Hmmm…… I also refuse to consider life as something meaningless."),MSGBOX_AUTOCLOSE)    
}

    speakername("Xylar")
    msgbox(format("Do you think he…… was a human?"),MSGBOX_YESNO)
if(var(VAR_RESULT)==YES){
    speakername("Xylar")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_SHOCK}Human…?\pMaybe you're right.{PAUSE_UNTIL_PRESS} But if that's true…\pThen what kind of human carries a presence like that?"),MSGBOX_AUTOCLOSE)    
}
else{
    speakername("Xylar")
    msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_NORM}No… you're right.\pNo human could stand there and make even Mewtwo hesitate.{PAUSE_UNTIL_PRESS}\pWhatever he is… he's not like us."),MSGBOX_AUTOCLOSE)    
}

