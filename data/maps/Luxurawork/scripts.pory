} elif (var(LUXURA_TRAGEDY) > 10) {
	removeobject(1)
	removeobject(2)
	removeobject(3)
	removeobject(4)
	addobject(5)
	addobject(6)
	addobject(7)
	addobject(8)
	addobject(9)
	addobject(10)
	addobject(12)
	removeobject(11)
	addobject(13)
	addobject(14)
	addobject(15)
	addobject(16)
	setflag(FLAG_UNUSED_0x4A7)
	setflag(FLAG_UNUSED_0x4A8)
	clearflag(FLAG_UNUSED_0x4A6)
    setflag(FLAG_UNUSED_0x49A)
}

}

script dad {
	if(!flag(FLAG_UNUSED_0x49D)){
	lockall
	faceplayer
	speakername(name3)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}Xylar, all set for the academy?"))
	speakername(Xylar1)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}Yeah, Dad.\pI've got everything packed."))
	speakername(name3)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}Good to hear.\pRemember you don't need to worry about living up to our name.\pI want you to have a life full of joy and discovery.")) 
	speakername(Xylar1)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}Thanks Dad, I'll try to make the most of it."))
    speakername(name3)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_JOY}{COLOR BLUE}{SHADOW LIGHT_GRAY}That's all I ask, son.")) 
    removefieldmugshot
	waitmessage
	applymovement(4,exclam)
	speakername(name3)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}Oh, Xylar, before I forget-\pAn old friend of mine from the academy will be visiting near the end of this month.\pHe's been very curious to meet you."))
	speakername(Xylar1)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_QUESTION}{COLOR BLUE}{SHADOW LIGHT_GRAY}Friend?\pFrom the academy, You didn't tell me about this."))
	speakername(name3)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_JOY}{COLOR BLUE}{SHADOW LIGHT_GRAY}Hahaha I meant to but it slipped my mind.\pHe wanted to meet you before the start.\pDon't worry he's just coming to make sure you're comfortable and ready."))
	speakername(Xylar1)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_XYLAR}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}Alright, I guess I'd better be on my best behaviour, then."))
	speakername(name3)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}Just be yourself, son. That's all he'll want to see."))
	removefieldmugshot
    releaseall
	setflag(FLAG_UNUSED_0x49D)}
	else{
	lockall
	faceplayer
	speakername(name3)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}I have some work to do.\pGo ahead and join your mother."))
	removefieldmugshot
	releaseall
	}
}
