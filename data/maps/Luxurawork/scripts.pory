script trageflash {
	lockall
	special(SpawnCameraObject)
	playse(SE_PIN)
	applymovement(7,exclam)
	waitmovement(0)
    applymovement(0x7F,mo)
	waitmovement(0)
	speakername(surge)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_SURGE}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}Looks like I'm down to my last one…"))
	waitmessage
	closemessage
	speakername(name3)
	msgbox("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}Hoopa…\pFinish it.")
	waitmessage
	closemessage
	playmoncry(SPECIES_HOOPA_UNBOUND,CRY_MODE_ROAR_1)
	speakername("Hoopa")
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_UNBOUND}{EMOTE_NORMAL}Hooo...paaa!"))
	waitmessage
	closemessage
	playse(SE_M_TELEPORT)
	applymovement(11,portal_open)
	waitmovement(0)
	applymovement(8,moves(face_up set_invisible))
	waitmovement(0)
	playse(SE_WARP_IN)
	waitse
	playse(SE_M_TELEPORT)
	applymovement(11,portal_close)
	waitmovement(0)
	applymovement(8,finish)
	applymovement(11,finish)
	waitmovement(0)
	playse(SE_M_TELEPORT)
	applymovement(11,portal_open)
	waitmovement(0)
	playse(SE_WARP_OUT)
	waitse
	applymovement(8,visible)
	waitmovement(0)
	playse(SE_M_TELEPORT)
	applymovement(11,invisible)
    applymovement(8,atk)
	playse(SE_M_TAKE_DOWN)
	applymovement(9,faint)
	waitmovement(0)
	playmoncry(SPECIES_RAICHU,CRY_MODE_FAINT)
	waitbuttonpress
	speakername(surge)
	msgbox("{CREATE_MUGSHOT}{MUGSHOT_SURGE}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}You're strong… stronger than I expected.")
	closemessage
	speakername(name3)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_QUESTION}{COLOR BLUE}{SHADOW LIGHT_GRAY}You call that strength?"))
	removefieldmugshot
	speakername(name3)
	msgbox(format("{CREATE_MUGSHOT}{MUGSHOT_AURTHER}{EMOTE_NORMAL}{COLOR BLUE}{SHADOW LIGHT_GRAY}You're pitiful, Surge.\pThis is where it ends."))
	waitmessage
    closemessage
	playse(SE_PIN)
	applymovement(14,moves(emote_exclamation_mark))
	waitmovement(0)
	playse(SE_M_DOUBLE_TEAM)
	applymovement(12,moves(spy_vanish set_visible spy_vanish face_down))
	waitmovement(12)
	playse(SE_M_BITE)
	waitse
	applymovement(8,run)
	playbgm(MUS_HG_ENCOUNTER_SAGE,TRUE)
	fadescreen(FADE_TO_BLACK)
	msgbox(format("{COLOR BLUE}{SHADOW LIGHT_GRAY}You spoke too soon,\nAurther."))
	closemessage
	applymovement(13,visible)
	applymovement(14,invisible)
    fadescreen(FADE_FROM_BLACK)
	applymovement(6,exclam)
	waitmovement(0)
    fadescreen(FADE_FROM_BLACK)
	fadescreenspeed(FADE_TO_WHITE,4)
	fadescreenspeed(FADE_FROM_WHITE,4)
	fadescreenspeed(FADE_TO_WHITE,4)
	fadescreenspeed(FADE_FROM_WHITE,4)
	fadescreen(FADE_TO_BLACK)
	setvar(LUXURA_TRAGEDY,23)
	warpsilent(MAP_LUXURA_INDOOR,14,33)
	
}
