} elif (var(LUXURA_TRAGEDY) >= 5 && var(LUXURA_TRAGEDY) <= 10) {
    addobject(4)
	removeobject(5)
	removeobject(6)
	removeobject(7)
	removeobject(8)
	removeobject(9)
	removeobject(10)
	removeobject(11)
	removeobject(12)
	removeobject(14)
	removeobject(13)
	removeobject(15)
	removeobject(16)
	removeobject(17)
	setflag(FLAG_UNUSED_0x4A6)
